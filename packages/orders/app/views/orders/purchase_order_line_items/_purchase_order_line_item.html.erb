<div id="<%= dom_id purchase_order_line_item %>">
  <p>
    <strong>Line number:</strong>
    <%= purchase_order_line_item.line_number %>
  </p>

  <p>
    <strong>Sku code:</strong>
    <%= purchase_order_line_item.sku_code %>
  </p>

  <p>
    <strong>Sku description:</strong>
    <%= purchase_order_line_item.sku_description %>
  </p>

  <p>
    <strong>Uom:</strong>
    <%= purchase_order_line_item.uom %>
  </p>

  <p>
    <strong>Ordered qty:</strong>
    <%= purchase_order_line_item.ordered_qty %>
  </p>

  <p>
    <strong>Received qty:</strong>
    <%= purchase_order_line_item.received_qty %>
  </p>

  <p>
    <strong>Sku holdcode:</strong>
    <%= purchase_order_line_item.sku_holdcode %>
  </p>

  <p>
    <strong>Sku type:</strong>
    <%= purchase_order_line_item.sku_type %>
  </p>

  <p>
    <strong>Purchase orders:</strong>
    <%= purchase_order_line_item.purchase_orders_id %>
  </p>

</div>
